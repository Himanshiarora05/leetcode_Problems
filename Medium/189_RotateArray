class Solution {
public:
    void reversePair(int i,int j,vector<int>& v){
        while(i<j){
            int temp= v[i];
            v[i] = v[j];
            v[j] = temp;
            i++;
            j--;
        }
        return;

    }
    void rotate(vector<int>& nums, int k) {
        int n=nums.size();
        k=k%n;
        reversePair(0,n-k-1,nums);
        reversePair(n-k,n-1,nums);
       reversePair(0,n-1,nums);
       for(int i=0;i<n;i++) cout<<nums[i];


    }
};
